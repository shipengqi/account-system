    "@angular/forms": "^18.1.1",
    "@angular/platform-browser": "^18.1.1",
    "@angular/platform-browser-dynamic": "^18.1.1",
    "@angular/router": "^18.1.1",
    "@antv/g2": "^5.2.2",
    "@delon/chart": "^18.1.0",
    "@ngx-translate/core": "^15.0.0",
    "deepmerge-ts": "^7.0.3",
    "file-saver": "^2.0.5",
    "moment": "^2.30.1",
    "ng-zorro-antd": "^18.1.0",
    "ngx-translate-multi-http-loader": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
