{
  "name": "as-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --proxy-config proxy-local.config.js --host 0.0.0.0",
    "build": "ng build --configuration production --deploy-url /static/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser edge --spec './cypress/e2e/regression/**/*'",
    "cy:run:headed": "cypress run --browser edge --headed --spec './cypress/e2e/regression/**/*'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.1",
    "@angular/common": "^18.1.1",
